    "import time\n",
    "\n",
    "zumi = Zumi()\n",
    "\n",
    "for i in range(0,10):\n",
    "    # battery level is updated every 500ms or half a second\n",
    "    time.sleep(0.5)\n",
    "    battery = zumi.get_battery_voltage()\n",
    "    print(battery)"
